{
  "name": "vibe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Ren√© Weiberlenn",
  "description": "Vibe - Self-Care and Mental Health",
  "license": "MIT",
  "keywords": [
    "typescript",
    "prettier",
    "eslint",
    "react",
    "vite",
    "tailwindcss",
    "eslint-plugin-react",
    "prettier-plugin-tailwindcss"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0 --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "npm run format"
    ],
    "*.{json,yml,yaml}": [
      "npm run format"
    ]
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.13.0",
    "tailwindcss": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "terser": "^5.46.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1"
  }
}
